<!-- Main Content -->
<section class="section">
  <div class="section-header">
    <h1>Video List</h1>
    <!-- <button class="btn btn-primary ml-2" onclick="history.back()"><i class="fa fa-arrow-left"
        aria-hidden="true"></i></button> -->

  </div>
  <div class="row" id="tableID">
    <div class="col-12">
      <!-- <div class="card-header"> -->
      <!-- <h4 class="card-title">Videos Table</h4> -->
      <div class="text-right">
        <a class="btn btn-primary mb-5" href="/admin/add_video">Add New</a>
      </div>
      <!-- </div> -->
      <div class="row">
        <% videos.forEach((data, i)=>{%>
          <div class="col-lg-4 col-md-6 col-12">
            <div class="card" style="width: 18rem">
              <video src="/images/videos/<%= data?.video%>" controls></video>
              <div class="card-body">
                <div class="d-flex align-items-center">
                  <h5 class="card-title mb-0">
                    <p style="color: #333; font-weight: bold;">
                    <%= data?.title %></p>
                  </h5>
                  <a href="/admin/edit_video/<%= data?.id %>" class="edit de ml-1"><i class="fas fa-edit" style="font-size: 12px;"></i></a>
                  <a onclick="deleteVideo('<%= data?.id %>')" class="delete de ml-2"><i class="fas fa-trash-alt" style="font-size: 12px;"></i></a>
                </div>
                

                <p class="card-text">
                  <%= data?.description%>
                </p>
                <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
              </div>
            </div>
          </div>
          <%})%>
      </div>
    </div>
  </div>
</section>

<!-- main content End -->
<script src="../assets/js/myScripts.js"></script>
<script>
  imgInp.onchange = (evt) => {
    const [file] = imgInp.files;
    if (file) {
      blah.src = URL.createObjectURL(file);
    }
  };
</script>