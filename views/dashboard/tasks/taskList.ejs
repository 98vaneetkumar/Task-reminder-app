<!-- Main Content -->
<section class="section">
  <div class="section-header">
    <h1>Tasks List</h1>
    <!-- <button class="btn btn-primary ml-2" onclick="history.back()"><i class="fa fa-arrow-left"
        aria-hidden="true"></i></button> -->

  </div>
  <div class="row" id="tableID">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <% if(pageType==1){%>
            <h4 class="card-title">Pre Created Tasks Table</h4>
            <a href="/admin/create_task" class="btn btn-success">Add</a>
            <%}else{%>
              <h4 class="card-title">User's Created Tasks Table</h4>
              <%}%>
        </div>
        <div class="table-responsive px-3">

          <table id="table_User_get" class="table table-striped table-bordered" style="width: 100%">
            <thead>
              <tr>
                <th>Sr.No</th>
                <th>Task Tittle</th>

                <th>User Name</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <% tasks.forEach(( data, i)=>{%>
                <tr>
                  <td>
                    <%= i+1%>
                  </td>
                  <td>
                    <%= data?.title%>
                  </td>


                  <td>
                    <%= data?.user?.name%>
                  </td>

                  <td>
                    <a class="btn btn-success text-white " href="/admin/view_task/<%= data?.id%>"><i
                        class="fas fa-eye"></i></a>
                    <a class="btn btn-danger  text-white deletePlan" onclick="deleteUsersTask('<%= data?.id %>')">
                      <i class="fas fa-trash-alt"></i>
                    </a>
                  </td>
                </tr>
                <% }) %>
            </tbody>
          </table>

        </div>
      </div>
    </div>
  </div>


</section>

<!-- main content End -->
<script src="../assets/js/myScripts.js"></script>